@{
    ViewData["Title"] = "Milionerzy - test logiki";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Who Wants to Be a Millionaire? - Main Page</title>
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/prizeTable.css" />
    <link rel="stylesheet" href="~/css/lifelines.css" />

</head>
<body>
    <div id="container">
        <div id="left">
            <div id="mobileDiv">MOBILE</div>
            <div id="questionWindow"> </div>
            <div id="buttons">
                <div><button id="btnA">A</button></div>
                <div><button id="btnB">B</button></div>
                <div><button id="btnC">C</button></div>
                <div><button id="btnD">D</button></div>
                <div><button id="btnResign">Resign</button></div>
            </div>
        </div>
        <div id="right">
            <div id="lifelines">
                <div id="phone">
                    <button id="btnPhone">Phone</button>
                </div>
                <div id="audience">
                    <button id="btnAudience">Audience</button>
                </div>
                <div id="fiftyFifty">
                    <button id="btnFiftyFifty">50/50</button>
                </div>
            </div>
            <div id="prizeTable"></div>
        </div>
    </div>
    <script src="~/js/questionWindow.js"></script>
    <script src="~/js/buttons.js"></script>
    <script src="~/js/balance.js"></script>
    <script src="~/js/randomNumberGenerator.js"></script>
    <script src="~/js/prizeTable.js"></script>
    <script src="~/js/lifelines.js"></script>
    <script src="~/js/game.js"></script>
</body>
</html>